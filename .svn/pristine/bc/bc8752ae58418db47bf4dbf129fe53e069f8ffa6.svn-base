package com.threegrand.controller.flex;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
@RequestMapping("flex")
public class FlexController {
	
	
	@RequestMapping("/mapLocation")
	public String mapLocation(){
		return "/flex/maplocation";
	}
	
	@RequestMapping("/mapListener")
	public String mapListener(){
		return "/flex/maplistener";
	}
	
	@RequestMapping("/mapHistoryOfCheck")
	public String mapHistoryOfCheck(){
		return "/flex/maphistoryofcheck";
	}

}
