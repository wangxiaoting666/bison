#ovrview .left .text{width:445px; height:154px;padding-top:15px;}
#ovrview .left .text p{ text-indent:25px; color:#001142;}
#ovrview .center{ float:left; width:342px; margin-left:15px;}
#ovrview .center .title{border-bottom:#001142 solid 2px; height:34px; padding-top:1px;}
#ovrview .center .title>h2{ margin-left:27px; background:url(../img/biao2.gif) no-repeat top left; color:#0f1f4d;font-weight:bold;height:35px; width:133px; text-align:right; line-height:35px; font-size:18px;}
#ovrview .center .text{width:328px; height:170px; padding-left:12px; margin-top:35px; background:url(../img/menuq.gif) no-repeat center top;}
#ovrview .center .text p{ width:94px;margin:auto;color:#001142; font-size:18px; font-weight:bold; padding-top:9px;}
#ovrview .center ul{ }
