#work  .workPagination{font-family:'微软雅黑';font-size:14px; padding: 30px 0; margin: 0 auto; text-align: center;}

#work .scholarships{ height:356px;border:#f2f1f1 solid 2px; margin-top:1px; padding:10px 13px 0px 6px;}
