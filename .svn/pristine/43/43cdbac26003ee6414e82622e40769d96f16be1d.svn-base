/*start ovrview */
#ovrview{ width:1003px; margin:0px auto; height:460px; padding-top:10px; padding-bottom:30px;}
#ovrview>h2{ color:#0f1f4d; font-size:14px; padding-left:43px; padding-bottom:14px; font-weight:bold;}
#ovrview .left{ float:left; width:498px;}
#ovrview .left ul{ border-bottom:#001142 solid 2px; height:35px;}
#ovrview .left ul div{ float:left; width:43px; height:34px; background: url(../img/biao1.gif) no-repeat right bottom; margin-right:9px;}
#ovrview .left ul li{ cursor:pointer; font-size:18px; float:left; width:104px; height:35px; background-color:#0a3e78; text-align:center; line-height:35px; font-weight:bold;}
#ovrview .left ul li a{color:#FFF; }
#ovrview .left .picturesKJ{ width:445px; padding:14px 0 0 34px; }
