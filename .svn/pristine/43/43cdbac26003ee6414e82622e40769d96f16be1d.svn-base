#work .scholarships .ul-menu .li1{float:left; background: #FFF url(../img/work-2.gif) no-repeat bottom right;}
#work .scholarships .ul-menu .li1 a{ display:inline-block; color:#494949; font-size:18px; font-weight:bold;  width:163px; height:29px; line-height:29px; text-align:center;}
#work .scholarships .ul-menu .li2{ float:left; background: #da0101 url(../img/work-2.gif) no-repeat bottom right; }
#work .scholarships .ul-menu .li2 a{ display:inline-block;color:#FFF; font-size:18px; font-weight:bold;  width:163px; height:29px; line-height:29px; text-align:center;}
#work .scholarships .ul-menu .li-end{  float:right; font-size:14px; font-weight:bold; width:74px; height:29px; line-height:29px; text-align:center;}
